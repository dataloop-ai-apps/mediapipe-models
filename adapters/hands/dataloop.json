{
  "name": "mp-hands",
  "displayName": "Media Pipe Hands model",
  "version": "0.0.5",
  "scope": "public",
  "description": "Media Pipe Hands model",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/mediapipe-models.git",
    "gitTag": "0.0.5"
  },
  "attributes": {
    "Provider": "Google",
    "Category": "Model",
    "License": "Apache 2.0",
    "Media Type": "Image",
    "Computer Vision": "Object Detection"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "media-pipe-hands-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "docker.io/yakirinven/mediapipe_image",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "media-pipe-hands-module",
        "entryPoint": "model_adapter.py",
        "className": "MediapipeModelAdapter",
        "computeConfig": "media-pipe-hands-deploy",
        "description": "Media Pipe Hands Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "List of annotations"
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Media Pipe Hands API"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "The input dataset of the items required for prediction."
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "The DQL in json format to get all the items required for prediction."
              }
            ],
            "output": [],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Inference function of the model on a dataset."
          }
        ]
      }
    ],
    "models": [
      {
        "name": "media-pipe-hands",
        "moduleName": "media-pipe-hands-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "model_name": "hands",
          "model_config": {
            "static_image_mode": true,
            "max_num_hands": 2,
            "min_detection_confidence": 0.5
          }
        },
        "description": "Media Pipe Hands model"
      }
    ]
  }
}
